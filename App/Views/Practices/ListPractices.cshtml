@model IEnumerable<Practices>
@{ Layout = "_Layout"; ViewBag.Title = "All Reservations";}

<h2>All Practices</h2>
<table class="table table-sm table-striped table-bordered m-2">
    <thead><tr><th>ID</th><th>Name</th><th>Address</th><th>City</th><th>Zip Code</th></tr></thead>
    <tbody>
    @foreach (var r in Model)
    {
        <tr>
            <td>@r.PtcId</td>
            <td>@r.PtcName</td>
            <td>@r.PtcAddress</td>
            <td>@r.PtcCity</td>
            <td>@r.PtcZipCode</td>
        </tr>
    }
    </tbody>
</table>